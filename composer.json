{
    "name": "permafrost-dev/laravel-current-route",
    "description": "Helpers for retrieving information about the current route",
    "keywords": [
        "laravel",
        "routes"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "^7.0",
        "illuminate/routing": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "nunomaduro/phpinsights": "^1.14",
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Permafrost\\CurrentRoute\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Permafrost\\CurrentRoute\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "vendor/bin/php-cs-fixer fix ./src",
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
